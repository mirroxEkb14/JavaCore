<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFx.sceneBuilder.VideoView.Controller">
   <children>
      <MediaView fx:id="myMediaView" fitHeight="300.0" fitWidth="500.0" />
      <Button fx:id="pauseButton" layoutX="224.0" layoutY="329.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
      <Button fx:id="playButton" layoutX="146.0" layoutY="329.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
      <Button fx:id="resetButton" layoutX="310.0" layoutY="329.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
   </children>
</AnchorPane>
